<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.palesd.gui.view_lists.ViewListsController">
  <children>
    <ListView fx:id="eventList" layoutX="14.0" layoutY="46.0" onMouseClicked="#handleSelectedEventAction" orientation="VERTICAL" prefHeight="335.0" prefWidth="362.0" />
    <Label layoutX="14.0" layoutY="14.0" text="Lists">
      <font>
        <Font size="24.0" fx:id="x2" />
      </font>
    </Label>
    <TableView id="eventGuests" fx:id="eventGuestTable" layoutX="400.0" layoutY="14.0" prefHeight="452.0" prefWidth="386.0">
      <columns>
        <TableColumn editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="136.0" resizable="false" text="First Name" fx:id="firstNameCol" />
        <TableColumn editable="false" maxWidth="5000.0" minWidth="10.0" prefWidth="140.0" resizable="false" text="Last Name" fx:id="lastNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="110.0" text="Number" fx:id="numberCol" />
      </columns>
    </TableView>
    <Button layoutX="300.0" layoutY="409.0" mnemonicParsing="false" text="Print">
      <font>
        <Font size="20.0" fx:id="x1" />
      </font>
    </Button>
    <Button id="cancelButton" fx:id="exitButton" font="$x1" layoutX="14.0" layoutY="409.0" mnemonicParsing="false" onMouseClicked="#handleExitButtonAction" text="Exit" />
  </children>
</AnchorPane>
