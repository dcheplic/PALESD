<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.palesd.gui.main.MasterListController">
  <children>
    <Button fx:id="exitButton" layoutX="14.0" layoutY="426.0" mnemonicParsing="false" onMouseClicked="#handleExitButtonAction" text="Exit">
      <font>
        <Font size="20.0" fx:id="x1" />
      </font>
    </Button>
    <Button fx:id="deleteButton" font="$x1" layoutX="608.0" layoutY="426.0" mnemonicParsing="false" onMouseClicked="#handleDeleteButtonAction" text="Delete From List" />
    <TableView fx:id="masterListTable" layoutX="14.0" layoutY="53.0" onMouseClicked="#handleSelectedItemAction" prefHeight="361.0" prefWidth="772.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="276.0" text="First Name" fx:id="firstNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="276.0" text="Last Name" fx:id="lastNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="219.0" text="Number" fx:id="numberCol" />
      </columns>
    </TableView>
    <Label layoutX="14.0" layoutY="14.0" text="Master List">
      <font>
        <Font size="24.0" />
      </font>
    </Label>
  </children>
</AnchorPane>
