<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="160.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.palesd.gui.main.MasterListStartController">
  <children>
    <Label layoutX="14.0" layoutY="14.0" text="Enter Admin Code:">
      <font>
        <Font size="24.0" />
      </font>
    </Label>
    <Button fx:id="exitButton" layoutX="14.0" layoutY="111.0" mnemonicParsing="false" onMouseClicked="#handleExitButtonAction" text="Exit">
      <font>
        <Font size="20.0" fx:id="x1" />
      </font>
    </Button>
    <Button fx:id="beginButton" font="$x1" layoutX="302.0" layoutY="111.0" mnemonicParsing="false" onMouseClicked="#handleBeginButtonAction" text="Begin" />
    <TextField fx:id="cardField" layoutX="14.0" layoutY="60.0" onAction="#onEnter" prefHeight="32.0" prefWidth="372.0" />
  </children>
</AnchorPane>
