<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.palesd.gui.meeting.MeetingReportController">
  <children>
    <TableView fx:id="attendanceTable" layoutX="16.0" layoutY="48.0" prefHeight="370.0" prefWidth="770.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="182.0" text="First Name" fx:id="firstNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="196.0" text="Last Name" fx:id="lastNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="216.0" text="Number" fx:id="numberCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="177.0" text="Time" fx:id="timeCol" />
      </columns>
    </TableView>
    <Button fx:id="exitButton" layoutX="16.0" layoutY="431.0" mnemonicParsing="false" onMouseClicked="#handleExitButtonAction" text="Exit">
      <font>
        <Font size="20.0" fx:id="x1" />
      </font>
    </Button>
    <Button font="$x1" layoutX="710.0" layoutY="431.0" mnemonicParsing="false" text="Print" />
    <Label layoutX="16.0" layoutY="16.0" text="Attendance List for ">
      <font>
        <Font size="24.0" fx:id="x2" />
      </font>
    </Label>
    <Label fx:id="meetingNameLabel" font="$x2" layoutX="223.0" layoutY="16.0" text="MEETING" />
  </children>
</AnchorPane>
