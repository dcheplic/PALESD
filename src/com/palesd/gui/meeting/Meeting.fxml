<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="480.0" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.palesd.gui.meeting.MeetingController">
  <children>
    <TableView fx:id="attendanceTable" layoutX="418.0" layoutY="44.0" prefHeight="413.0" prefWidth="359.0">
      <columns>
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="113.0" text="First Name" fx:id="firstNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="111.0" text="Last Name" fx:id="lastNameCol" />
        <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="134.0" text="Number" fx:id="numberCol" />
      </columns>
    </TableView>
    <Label layoutX="420.0" layoutY="12.0" text="Attendance List">
      <font>
        <Font size="24.0" fx:id="x1" />
      </font>
    </Label>
    <Label font="$x1" layoutX="16.0" layoutY="12.0" text="Card Number" />
    <TextField fx:id="cardField" layoutX="16.0" layoutY="44.0" onAction="#onEnter" prefHeight="39.0" prefWidth="372.0" />
    <Button fx:id="exitButton" layoutX="16.0" layoutY="422.0" mnemonicParsing="false" onMouseClicked="#handleExitButtonAction" text="Exit">
      <font>
        <Font size="20.0" />
      </font>
    </Button>
  </children>
</AnchorPane>
